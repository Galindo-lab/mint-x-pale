<?xml version="1.0" encoding="UTF-8"?>

<style-scheme id="mint-x-pale" _name="Mint X Pale" version="1.0">

	<author>vlad1777d</author>
	<_description>Mint X Pale</_description>


	<color name="text_grey"      value="#dcdcdc"/>
	<color name="text_dark_grey" value="#999999"/>
	<color name="grey_soft"      value="#404040"/>
	<color name="grey_darkest"   value="#242424"/>
	<color name="yellow_sand"    value="#e0ce61"/>


	<style name="bracket-match"    foreground="yellow_sand" background="grey_darkest" bold="true"/>
	<style name="bracket-mismatch" foreground="#e53e3e"     background="grey_soft"    underline="true"/>
	<style name="current-line" background="#1a1a1a"/>
	<style name="cursor" foreground="text_grey"/>
	<style name="line-numbers" foreground="text_dark_grey" background="grey_darkest"/>
	<style name="draw-spaces" foreground="#6a6a6a"/>
	<style name="search-match" foreground="#ffffff" background="grey_soft" bold="true" underline="false"/>

	<style name="text" foreground="text_grey" background="grey_soft"/>
	<style name="right-margin" foreground="#c8f0a0"/>

	<style name="css:at-rules" foreground="#ad8dff"/>
	<style name="css:color" foreground="text_grey"/>
	<style name="css:keyword" foreground="#99cc99"/>

	<style name="def:base-n-integer" foreground="yellow_sand"/>
	<style name="def:boolean" foreground="yellow_sand"/>
	<style name="def:builtin" foreground="yellow_sand"/>
	<style name="def:character" foreground="yellow_sand"/>
	<style name="def:comment" foreground="#999999"/>
	<style name="def:complex" foreground="yellow_sand"/>
	<style name="def:decimal" foreground="yellow_sand"/>
	<style name="def:doc-comment" foreground="#999999"/>
	<style name="def:doc-comment-element" foreground="#999999"/>
	<style name="def:error" foreground="#cdcdcd" background="#83acff"/>
	<style name="def:floating-point" foreground="yellow_sand"/>
	<style name="def:function" foreground="#83acff"/>
	<style name="def:identifier" foreground="#83acff"/>
	<style name="def:keyword" foreground="#ad8dff"/>
	<style name="def:note" foreground="#999999"/>
	<style name="def:number" foreground="yellow_sand"/>
	<style name="def:operator" foreground="#66cccc"/>
	<style name="def:preprocessor" foreground="yellow_sand"/>
	<style name="def:reserved" foreground="#4899cc"/>
	<style name="def:shebang" foreground="#999999"/>
	<style name="def:special-char" foreground="yellow_sand"/>
	<style name="def:special-constant" foreground="yellow_sand"/>
	<style name="def:statement" foreground="#ad8dff"/>
	<style name="def:string" foreground="#99cc99"/>
	<style name="def:type" foreground="yellow_sand"/>

	<style name="html:dtd" foreground="#99cc99"/>
	<style name="html:tag" foreground="#83acff"/>

	<style name="js:function" foreground="#83acff"/>

	<style name="perl:builtin" foreground="#83acff"/>
	<style name="perl:include-statement" foreground="#ad8dff"/>
	<style name="perl:special-variable" foreground="yellow_sand"/>
	<style name="perl:variable" foreground="#83acff"/>

	<style name="php:string" foreground="#99cc99"/>

	<style name="python:builtin-constant" foreground="#ad8dff"/>
	<style name="python:builtin-function" foreground="#83acff"/>
	<style name="python:module-handler" foreground="#ad8dff"/>
	<style name="python:special-variable" foreground="#ad8dff"/>

	<style name="ruby:attribute-definition" foreground="#ad8dff"/>
	<style name="ruby:builtin" foreground="#83acff"/>
	<style name="ruby:class-variable" foreground="#83acff"/>
	<style name="ruby:constant" foreground="#83acff"/>
	<style name="ruby:global-variable" foreground="#83acff"/>
	<style name="ruby:instance-variable" foreground="#83acff"/>
	<style name="ruby:module-handler" foreground="#ad8dff"/>
	<style name="ruby:predefined-variable" foreground="yellow_sand"/>
	<style name="ruby:regex" foreground="#99cc99"/>
	<style name="ruby:special-variable" foreground="#ad8dff"/>
	<style name="ruby:symbol" foreground="#99cc99"/>

	<style name="rubyonrails:attribute-definition" foreground="#ad8dff"/>
	<style name="rubyonrails:block-parameter" foreground="yellow_sand"/>
	<style name="rubyonrails:builtin" foreground="#83acff"/>
	<style name="rubyonrails:class-inherit" foreground="#99cc99"/>
	<style name="rubyonrails:class-name" foreground="yellow_sand"/>
	<style name="rubyonrails:class-variable" foreground="#83acff"/>
	<style name="rubyonrails:complex-interpolation" foreground="yellow_sand"/>
	<style name="rubyonrails:constant" foreground="#83acff"/>
	<style name="rubyonrails:global-variable" foreground="#83acff"/>
	<style name="rubyonrails:instance-variable" foreground="#83acff"/>
	<style name="rubyonrails:module-handler" foreground="#ad8dff"/>
	<style name="rubyonrails:module-name" foreground="yellow_sand"/>
	<style name="rubyonrails:predefined-variable" foreground="yellow_sand"/>
	<style name="rubyonrails:rails" foreground="#83acff"/>
	<style name="rubyonrails:regex" foreground="#99cc99"/>
	<style name="rubyonrails:simple-interpolation" foreground="yellow_sand"/>
	<style name="rubyonrails:special-variable" foreground="#ad8dff"/>
	<style name="rubyonrails:symbol" foreground="#99cc99"/>

	<style name="xml:attribute-name" foreground="#83acff"/>
	<style name="xml:doctype" foreground="#f2777a"/>
	<style name="xml:element-name" foreground="#f2777a"/>
	<style name="xml:namespace" foreground="#f2777a"/>
	<style name="xml:tag" foreground="#f2777a"/>

</style-scheme>
